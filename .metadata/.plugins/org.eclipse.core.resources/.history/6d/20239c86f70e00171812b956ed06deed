package com.aaaaaaa;

public class Rectangle extends Shape  {
//	int chang=10;
//	int kuan=8;
	int M;
	int Z;
	public void Area(int kuan,int chang){
		M=chang*kuan;
		System.out.println("面积为："+M);
	}
    public void Perimete(int chang ,int kuan){
    	Z=(chang+kuan)*2;
   	System.out.println("周长为："+Z);
   }
}
