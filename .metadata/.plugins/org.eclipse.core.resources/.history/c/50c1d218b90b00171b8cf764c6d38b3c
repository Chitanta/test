package com.linp;

public class People {
	int age;
	String sex;
	static String name;
	public People(){
		age=20;
	}
	{
		sex="男";
	}
	static {
		name="阿姆斯特朗回旋加速喷气式阿姆斯特朗炮";
	}
	public static void main(String[] args) {
		People people=new People();
		System.out.println("age:"+people.age);
		System.out.println("sex"+people.sex);
		System.out.println("name:"+name);
	}
}
