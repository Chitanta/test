package com.linjiaq;
public class Lying {
	private String name="chichichi";
    public static int a=1;
    public static class Man{
    	String name="hahaha";
    	int a=2;
    }
    public void Outer(){
    	System.out.println("外部："+Lying.this.name);
    	System.out.println("内部："+a);
    	System.out.println("内部："+name);
    	System.out.println("外部："+Lying.a);
    }
}
